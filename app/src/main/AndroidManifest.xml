<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".ui.activity.EntryPoint"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KPKAudioLibrary"
        tools:targetApi="31">
        <activity
            android:name=".ui.activity.ExercisesTableActivity"
            android:exported="false"
            android:theme="@style/Theme.KPKAudioLibrary" />
        <activity
            android:name=".ui.activity.LessonsTableActivity"
            android:exported="false"
            android:theme="@style/Theme.KPKAudioLibrary" />
        <activity
            android:name=".ui.activity.BookTableActivity"
            android:exported="true"
            android:theme="@style/Theme.KPKAudioLibrary">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>